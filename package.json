{
  "name": "tslint-config-acamica",
  "version": "2.0.0",
  "description": "A TSLint config for Acamica Style",
  "main": "tslint.js",
  "files": [
    "tslint.js",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "lint": "standard",
    "test:gen": "GENERATE_ASSETS=true blue-tape test/index.js",
    "test:spec": "blue-tape test/index.js | tap-spec",
    "test": "npm run lint && npm run test:spec"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:acamica/tslint-config-acamica.git"
  },
  "keywords": [
    "tslint",
    "config",
    "typescript",
    "ts"
  ],
  "author": {
    "name": "Blake Embrey",
    "email": "hello@blakeembrey.com",
    "url": "http://blakeembrey.me"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "blue-tape": "^1.0.0",
    "tap-spec": "^4.1.1",
    "tslint": "5.7.0",
    "typescript": "2.5.3"
  },
  "dependencies": {
    "tslint-eslint-rules": "4.1.1"
  }
}
